{
    "manifest_version": 2,
    "name": "My Extension",
    "version": "1.0",
    "description": "browser addon for the BGA game CATAN",
    "permissions": ["activeTab", "scripting", "webNavigation", "<all_urls>"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentScript.js"]
        }
    ]
}
